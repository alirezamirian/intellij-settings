<application>
  <component name="LanguageInjectionConfiguration">
    <option name="INSTRUMENTATION" value="ASSERT" />
    <option name="LANGUAGE_ANNOTATION" value="org.intellij.lang.annotations.Language" />
    <option name="PATTERN_ANNOTATION" value="org.intellij.lang.annotations.Pattern" />
    <option name="SUBST_ANNOTATION" value="org.intellij.lang.annotations.Subst" />
    <option name="RESOLVE_REFERENCES" value="true" />
    <injection language="file-reference" injector-id="xml">
      <display-name>icon/@src</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("src")).withParent(xmlTag().withLocalName(string().equalTo("icon")).withNamespace(string().equalTo("http://www.w3.org/ns/widgets")))]]></place>
    </injection>
    <injection language="AngularJS" injector-id="xml">
      <display-name>li/@permission-only</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("permission-only")).withParent(xmlTag().withLocalName(string().equalTo("li")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
    <injection language="AngularJS" injector-id="xml">
      <display-name>md-autocomplete/@md-items</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("md-items")).withParent(xmlTag().withLocalName(string().equalTo("md-autocomplete")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
    <injection language="file-reference" injector-id="xml">
      <display-name>md-icon/@md-svg-icon</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("md-svg-icon")).withParent(xmlTag().withLocalName(string().equalTo("md-icon")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
    <injection language="file-reference" injector-id="xml">
      <display-name>md-icon/@md-svg-src</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("md-svg-src")).withParent(xmlTag().withLocalName(string().equalTo("md-icon")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
    <injection language="AngularJS" injector-id="xml">
      <display-name>slick/@settings</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("settings")).withParent(xmlTag().withLocalName(string().equalTo("slick")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
    <injection language="file-reference" injector-id="xml">
      <display-name>splash/@src</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("src")).withParent(xmlTag().withLocalName(string().equalTo("splash")).withNamespace(string().equalTo("http://www.w3.org/ns/widgets")))]]></place>
    </injection>
    <injection language="AngularJS" injector-id="xml">
      <display-name>th/@permission-only</display-name>
      <place><![CDATA[xmlAttribute().withLocalName(string().equalTo("permission-only")).withParent(xmlTag().withLocalName(string().equalTo("th")).withNamespace(string().equalTo("http://www.w3.org/1999/html")))]]></place>
    </injection>
  </component>
</application>